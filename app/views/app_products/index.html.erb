<h1>Listing app_products</h1>

<table>
  <tr>
    <th>App</th>
    <th>Product</th>
    <th>Min appversion</th>
    <th>Max appversion</th>
    <th>Whoisusing</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_products.each do |app_product| %>
  <tr>
    <td><%= app_product.app_id %></td>
    <td><%= app_product.product_id %></td>
    <td><%= app_product.min_appversion_id %></td>
    <td><%= app_product.max_appversion_id %></td>
    <td><%= app_product.whoisusing %></td>
    <td><%= link_to 'Show', app_product %></td>
    <td><%= link_to 'Edit', edit_app_product_path(app_product) %></td>
    <td><%= link_to 'Destroy', app_product, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App product', new_app_product_path %>
